{
    "name": "video-proxy-player",
    "version": "1.0.0",
    "description": "A simple proxy server to bypass CORS for HLS video streaming.",
    "main": "proxy-server.js",
    "scripts": {
        "start": "node proxy-server.js",
        "crawl-test": "node crawler.js 1 3",
        "crawl": "node crawler.js 1 50",
        "crawl-all": "node crawler.js 1 1000",
        "crawl-simple-test": "node simple-crawler.js 1 3",
        "crawl-simple": "node simple-crawler.js 1 50",
        "crawl-simple-all": "node simple-crawler.js 1 1000",
        "api-crawl-test": "node api-crawler.js 1 3",
        "api-crawl": "node api-crawler.js 1 50",
        "api-crawl-all": "node api-crawler.js 1 1000",
        "dynamic-api-crawl-test": "node dynamic-api-crawler.js 1 3",
        "dynamic-api-crawl": "node dynamic-api-crawler.js 1 50",
        "dynamic-api-crawl-all": "node dynamic-api-crawler.js 1 1000",
        "batch-crawl": "node batch-crawler.js crawl 1 999",
        "batch-crawl-test": "node batch-crawler.js crawl 1 10",
        "batch-retry": "node batch-crawler.js retry",
        "batch-status": "node batch-crawler.js status",
        "batch-summary": "node batch-crawler.js summary",
        "build-index": "node search-index.js --rebuild",
        "test-search": "node search-index.js --test"
    },
    "keywords": [
        "hls",
        "proxy",
        "cors",
        "crawler",
        "api",
        "search"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.11.0",
        "cheerio": "^1.1.2",
        "express": "^4.19.2",
        "puppeteer": "^22.15.0"
    }
}